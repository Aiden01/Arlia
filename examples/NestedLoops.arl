import dio;

func main() {
    for (i : integer = 0 to 11 | j : integer = 0 to 11)
        @dio:display.writeln(i.ToString() + " * " + j.ToString() + " = " (i * j).ToString);
}

`output:
    0 * 0 = 0
    1 * 1 = 1
    2 * 2 = 4
    3 * 3 = 9
    4 * 4 = 16
    5 * 5 = 25
    6 * 6 = 36
    7 * 7 = 49
    8 * 8 = 64
    9 * 9 = 81
    10 * 10 = 100
`
