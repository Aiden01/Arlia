type Person(firstname, lastname, age);
type Employee(salary, identity: Person);

let employee = new Employee(3600, new Person("Bob", "Marley", 36));

printfn("firstname: " ^ employee.identity.firstname);
printfn("lastname: " ^ employee.identity.lastname);
printfn("age: " ^ employee.identity.age.toString());
printfn("salary: " ^ employee.salary.toString());
